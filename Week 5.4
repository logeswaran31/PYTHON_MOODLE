s=input()
i=0
expanded=''
while i<len(s):
    char=s[i]
    if i+1<len(s) and s[i+1].isdigit():
        count=''
        while i+1<len(s) and s[i+1].isdigit():
            count=count+s[i+1]
            i=i+1
        count=int(count)
        expanded=expanded+char*count
        i=i+1
    else:
        expanded=expanded+char
        i=i+1
print(expanded)
